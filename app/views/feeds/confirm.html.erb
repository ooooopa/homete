<div class="container">
  <h1>この画像を投稿していいですか？</h1>
  <%= form_with(model:@feed, url:feeds_path, local:true) do |form| %>
    <%= image_tag(@feed.image.url) %>
    <%= hidden_field_tag :"feed[image]",@feed.image.url %>
    <%= hidden_field_tag :"cache[image]",@feed.image.cache_name %>
    <%= form.submit "投稿する", class: "btn btn-warning col-md-offset-6 col-md-2" %>
  <% end %>

  <%= form_with(model:@feed, url:new_feed_path, local:true, method:"get") do |form| %>
    <%= hidden_field_tag :"feed[image]",@feed.image.url %>
    <%= hidden_field_tag :"cache[image]",@feed.image.cache_name %>
    <%= form.submit "戻る", name: "back", class: "btn btn-warning col-md-offset-1 col-md-2" %>
  <% end %>
</div>